<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <cron_cleaner>
            <general>
                <enable>1</enable>
                <for_cache>
                    <enable_cache>1</enable_cache>
                    <cache_cron>* * * * *</cache_cron>
                </for_cache>
                <for_reindex>
                    <enable_reindex>1</enable_reindex>
                    <reindex_cron>* * * * *</reindex_cron>
                </for_reindex>
                <for_log>
                    <enable_log>1</enable_log>
                    <log_cron>* * * * *</log_cron>
                </for_log>
            </general>
        </cron_cleaner>
    </default>
</config>